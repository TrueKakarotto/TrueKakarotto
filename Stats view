                                                                                ["MouseHit"] = Echar:GetPrimaryPartCFrame() * CFrame.new(0, 5, 0),
                                                                                ["FaceMouse"] = true
                                                                            }
                                                                            
                    
                    local ohBoolean2 = true
                    local ohNil3 = nil
                    
                    game:GetService("ReplicatedStorage").Package.Events.BigBall:InvokeServer("Ultimate Spirit Bomb", ohBoolean2, ohNil3, ohTable2)
                end)
                    spawn(function ()
                        player = game.Players.LocalPlayer
                                                                             
                                                                             ohTable2 = {
                                                                                ["MouseHit"] = Echar:GetPrimaryPartCFrame() * CFrame.new(0, 5, 0),
                                                                                ["FaceMouse"] = true
                                                                            }
                                                                            
                    local ohBoolean2 = false
                    local ohBoolean3 = true
                    
                    game:GetService("ReplicatedStorage").Package.Events.BigBall:InvokeServer("Ultimate Spirit Bomb", ohBoolean2, ohBoolean3, ohTable2)
                end)
                    
                        
                    
                    
                    spawn(function ()
                        player = game.Players.LocalPlayer
                                                                             
                                                                             ohTable2 = {
                                                                                ["MouseHit"] = Echar:GetPrimaryPartCFrame(),
                                                                                ["FaceMouse"] = true
                                                                            }
                                                                            
                    
                    game:GetService("ReplicatedStorage").Package.Events.Hak:InvokeServer("Soul Punisher", ohTable2, "Blacknwhite27")
                end)
                    spawn(function ()
                        player = game.Players.LocalPlayer
                                                                             
                                                                             ohTable2 = {
                                                                                ["MouseHit"] = Echar:GetPrimaryPartCFrame(),
                                                                                ["FaceMouse"] = true
                                                                            }
                                                                            
                    
                    game:GetService("ReplicatedStorage").Package.Events.Hak:InvokeServer("Destruction", ohTable2, "Blacknwhite27")
                    end)
					end)
                    wait(.5)
				until getgenv().killp == false
			end
		end,
	})
	local followplayer = Tab2:CreateToggle({
		Name = "Follow [Player]",
		CurrentValue = false,
		Callback = function(Value)
			getgenv().followp = Value
			while getgenv().followp == true do
				task.wait()
				if getgenv().followp ~= true then
					break
				end
				repeat
					task.wait()
					pcall(function()
						local Echar = getgenv().SelectedPlayer.Character
						local myChar = game.Players.LocalPlayer.Character
						myChar.HumanoidRootPart.CFrame = Echar:GetPrimaryPartCFrame() * CFrame.new(0, 0, 2)
					end)
				until getgenv().followp == false
			end
		end,
	})
	local ToPlayer = Tab2:CreateButton({
		Name = "To [Player]",
		Callback = function()
			local	echar = getgenv().SelectedPlayer.Character
			local	myChar = game.Players.LocalPlayer.Character
			myChar.HumanoidRootPart.CFrame = echar:GetPrimaryPartCFrame() * CFrame.new(0, 0, 2)
		end,
	})
	local Section5 = Tab2:CreateSection("NPCs")
	local ChangeFace = Tab2:CreateButton({
		Name = "Appearance Change",
		Callback = function()
			if game.PlaceId == 5151400895 then
				return notify("Earth Only!")
			else
				task.spawn(function()
					game:GetService("ReplicatedStorage").Package.Events.Qaction:InvokeServer(workspace.Others.NPCs["Appearance Changer"])
				end)
			end
		end,
	})
	local Tab3 = Window:CreateTab("Forms", 4483362458)

    
	local Section6 = Tab3:CreateSection("Stacking")
	local Paragraph = Tab3:CreateParagraph({
		Title = "Available forms to stack",
		Content = "Kaioken, SSJ Kaioken, SSJ2 Majin, SSJ3, LSSJ, Mystic, Godly SSJ2"
	})
	local Dropdown = Tab3:CreateDropdown({
        Name = "Form",
        Options = {"Kaioken", "SSJ Kaioken", "SSJ2 Majin", "SSJ3", "LSSJ", "Mystic", "Godly SSJ2"},
        CurrentOption = {"Godly SSJ2"},
        MultipleOptions = false,
        Flag = "Dropdown1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
        Callback = function(Option)

            getgenv().tostack = tostring(Option[1])
            --print(getgenv().tostack)

        -- The function that takes place when the selected option is changed
        -- The variable (Option) is a table of strings for the current selected options
        end,
     })
	local Stack = Tab3:CreateButton({
		Name = "Stack",
		Callback = function()
			-- getgenv().tostack = tostring(Dropdown.CurrentOption[1])
			wait()
            print(getgenv().tostack)
			if getgenv().tostack == nil then
				notify("You have not selected a form")
			else
				if getgenv().AutoTransform == true then
					AutoTransforming:Set(false)
					task.wait(1)
					stack(getgenv().tostack)
					task.wait(1)
					AutoTransforming:Set(true)
				else
					stack(getgenv().tostack)
				end
			end
		end,
	})
	local Section7 = Tab3:CreateSection("Form Stuff")
	local InfStamina = Tab3:CreateToggle({
		Name = "No Form Energy Drain",
		CurrentValue = false,
		Flag = "Toggle8",
		Callback = function(Value)
			getgenv().togg3le = Value
			
			local event = game:GetService("ReplicatedStorage").Package.Events.drain
			local OldHook, hookmetamethod, getnamecallmethod = nil, hookmetamethod, getnamecallmethod
			OldHook = hookmetamethod(game, "__namecall", function(self, ...)
				local Got  = getnamecallmethod():lower()
				if (getgenv().togg3le) and self == event and Got:lower() == "fireserver" then
					return
				end
				return OldHook(self, ...)
			end)
		end,
	})
	
	local Tab4 = Window:CreateTab("Stats Edit", 4483362458)
	local Section8 = Tab4:CreateSection("Changing Stats")
	local Abbreviate = Tab4:CreateButton({
		Name = "Abbreviate Stats",
		Callback = function(Value)
			task.spawn(function()
				local suffixes = {
					"K",
					"M",
					"B",
					"T",
					"Qd",
					"Qn"
				}

				local function FormatValue(value)
					local negative = (tonumber(value) or 0) < 0
					if negative then
						value = -value
					end
			
					local str = value
					local formatted = false
			
					for i = #suffixes, 1, -1 do
						local divisor = 10 ^ (i * 3)
						if value >= divisor then
							str = math.floor(value / divisor * 100) / 100 .. suffixes[i]
							formatted = true
							break
						end
					end
			
					if not formatted then
						str = math.floor(value)
					end
			
					if negative then
						str = "-" .. str
					end
			
					return str
				end
			
				local isRunning = Value
				local breaks = Value
			
				local function UpdateUI()
					while isRunning do
						task.wait()
						if game:GetService("Players").LocalPlayer.Character then
							local humanoid = game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
							if humanoid and humanoid.Health > 0 then
								
			
								elements.ZeniValue.Text = "Zeni: " .. FormatValue(statValues.Zeni)
								elements.Strength.Text = "Strength: " .. FormatValue(statValues.StrengthValue)
								elements.Speed.Text = "Speed: " .. FormatValue(statValues.SpeedValue)
								elements.Defense.Text = "Defense: " .. FormatValue(statValues.DefenseValue)
								elements.Energy.Text = "Energy: " .. FormatValue(statValues.EnergyValue)
			
								if statValues.TransfromValue ~= "None" then
									statValues.Strength.Text = "Strength: " .. FormatValue(statValues.StrengthValue) .. " (+" ..
														FormatValue(statValues.FormStrength - statValues.StrengthValue) .. ")"
														statValues.Speed.Text = "Speed: " .. FormatValue(statValues.SpeedValue) .. " (+" ..
													 FormatValue(statValues.FormSpeed - statValues.SpeedValue) .. ")"
													 statValues.Defense.Text = "Defense: " .. FormatValue(statValues.DefenseValue) .. " (+" ..
													   FormatValue(statValues.FormDefense - statValues.DefenseValue) .. ")"
													   statValues.Energy.Text = "Energy: " .. FormatValue(statValues.EnergyValue) .. " (+" ..
													  FormatValue(statValues.FormEnergy - statValues.EnergyValue) .. ")"
								end
			
								statValues.healthbar.Text = "Health: " .. FormatValue(statValues.currentHealth) .. " / " .. FormatValue(statValues.maxHealth)
								statValues.energybar.Text = "Energy: " .. FormatValue(statValues.currentEnergy) .. " / " .. FormatValue(statValues.MaxEnergy)
							end
						else
							isRunning = false
						end
					end
				end
			
				task.spawn(UpdateUI)
			
				local function OnCharacterAdded(character)
					isRunning = true
					task.spawn(UpdateUI)
				end
			
				game:GetService("Players").LocalPlayer.Character.Humanoid.HealthChanged:Connect(function(value)
					local healthbar = game:GetService("Players").LocalPlayer.PlayerGui.Main.MainFrame.Bars.Health.TextLabel
					healthbar.Text = "Health: " .. FormatValue(value) .. " / " .. FormatValue(game:GetService("Players").LocalPlayer.Character.Humanoid.MaxHealth)
				end)
			
				local function OnCharacterRemoving()
					isRunning = false
				end
			
				game:GetService("Players").LocalPlayer.CharacterAdded:Connect(OnCharacterAdded)
				game:GetService("Players").LocalPlayer.CharacterRemoving:Connect(OnCharacterRemoving)
			end)
		end,
	})
	local flyoffon = Tab4:CreateToggle({
		Name = "Fly speed",
		CurrentValue = false,
		Flag = "Toggle9",
		Callback = function(Value)
			getgenv().onoffidk = Value
			task.wait()
			if getgenv().onoffidk ~= true then
				getgenv().settings['FLYING']['SPEED'] = nil
			end
		end,
	})

	local Flyspeedset = Tab4:CreateInput({
		Name = "Fly speed amount:",
		PlaceholderText = "Put a amount here.",
		RemoveTextAfterFocusLost = false,
		Callback = function(amount)
			getgenv().settings = {
				['FLYING'] = {
					['ON'] = getgenv().onoffidk;
					['SPEED'] = amount;
				};
			}
			local s = getgenv().settings['FLYING']['SPEED']
			while getgenv().settings['FLYING']['ON'] == true do
				game:GetService("RunService").Heartbeat:wait();
				getrenv()._G.FlightSpeed = getgenv().settings['FLYING']['SPEED'];
				if s ~= getgenv().settings['FLYING']['SPEED'] then
					break;
				end;
			end;
		end,
	})
	local Section9 = Tab4:CreateSection("Checking Stats")
	local Rebirthamountset = Tab4:CreateInput({
		Name = "Input number of rebirths",
		PlaceholderText = "Put a amount here.",
		RemoveTextAfterFocusLost = false,
		Callback = function(amount)
			getgenv().SelectedRebirthAmount = amount
		end,
	})
