	function UpdateText(c)
		elements.Selected.Text = c.Name
		local StatsQuest = getrenv()._G.x.GetRecommendedQuest(c)
		
		elements.Strength1.Text = Format(statValues.Strength)
		elements.Speed1.Text = Format(statValues.Speed)
		elements.Defense1.Text = Format(statValues.Defense)
		elements.Energy1.Text = Format(statValues.Energy)
		elements.Rebirths1.Text = statValues.Rebirth
		elements.Zeni1.Text = Format(statValues.StatsMoney)
		elements.Multiplier1.Text = game:GetService("ReplicatedStorage").Datas[tostring(c.UserId)].Rebirth.Value * 0.2 + 1
		elements.Allignment1.Text = game:GetService("ReplicatedStorage").Datas[tostring(c.UserId)].Allignment.Value
		elements.CurrentForm1.Text = c.Status.Transformation.Value
		elements.SelectedForm1.Text = c.Status.SelectedTransformation.Value
		elements.Quest1.Text = StatsQuest
	end
	
	elements.Select.FocusLost:Connect(function(a)
		if a then
			a = Findplayer(Select.Text)
			if a then
				while a and View.Visible do
					if not a then
						break
					end
					task.wait()
					UpdateText(game:GetService("Players"):FindFirstChild(a))
				end
			else
				Selected.Text = "Player not found"
			end
		end
	end) 
