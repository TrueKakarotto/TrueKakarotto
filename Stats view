	-- Scripts:
	
	local function DBYP_fake_script() -- View.Dragify 
		local script = Instance.new('LocalScript', View)
	
		local UserInputService = game:GetService("UserInputService")
		
		local gui = script.Parent
		
		dragging = false
		local dragInput
		local dragStart
		local startPos
		
		local function update(input)
			local delta = input.Position - dragStart
			gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
		
		gui.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				dragging = true
				dragStart = input.Position
				startPos = gui.Position
		
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)
		
		gui.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
		
		UserInputService.InputChanged:Connect(function(input)
			if input == dragInput and dragging then
				update(input)
			end
		end)
	end
	coroutine.wrap(DBYP_fake_script)()
	local function g(h)
		return h:gsub("%s+", "") or h
	end
	local function Findplayer(h, h2)
		if string.lower(h) == "me" then
			if h2 then
				return h2
			else
				return game.Players.Player.Name
			end
		else
			h = g(h)
			for m, n in pairs(game.Players:GetPlayers()) do
				if n.Name:lower():match("^" .. h:lower()) or n.DisplayName:lower():match("^" .. h:lower()) then
					return n.Name
				end
			end
		end
		return nil
	end
	
	function Format(n)
		local n = tostring(n)
		local f = {
			"",
			"K",
			"M",
			"B",
			"T",
			"Q",
			"Qd",
			"Qt",
			"Se",
			"Sp",
		}
		local s = n:sub(1, (#n - 1) % 3 + 1)
		local a = n:sub(#s + 1, #s + 1)
		local av = ""
		if tonumber(a) and #s > 0 and tonumber(a) > 0 then
			av = "." .. a
		end
		return s .. av .. f[math.floor((#n - 1) / 3) + 1]
	end

