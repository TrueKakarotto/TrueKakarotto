    local function GetPlayer(String)
        String = String:gsub("%s+", "")
        for _, v in pairs(game:GetService("Players"):GetPlayers()) do
            if v.Name:lower():match("^" .. String:lower()) or v.DisplayName:lower():match("^" .. String:lower()) then
                return v
            end
        end
        return nil
    end
	local Section4 = Tab2:CreateSection("Other Players")
	local PlayerSelector = Tab2:CreateInput({
		Name = "Select a player",
		PlaceholderText = "Type the name here!",
		RemoveTextAfterFocusLost = false,
		Callback = function(tblud)
			getgenv().SelectedPlayer = GetPlayer(tblud)
			task.wait()
			if getgenv().SelectedPlayer == nil then
				game:GetService("StarterGui"):SetCore("SendNotification", {
					Title = "Script",
					Text = "Player not found!",
					Icon = nil,
					Duration = 3,
				})
			else
				game:GetService("StarterGui"):SetCore("SendNotification", {
					Title = "Script",
					Text = "You have selected " .. getgenv().SelectedPlayer.Name,
					Icon = nil,
					Duration = 3,
				})
			end
		end,
	})
