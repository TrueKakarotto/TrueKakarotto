		Name = "Check Rebith Multiplier",
		Callback = function()
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Script",
				Text = "" .. getgenv().SelectedRebirthAmount * 0.2 + 1,
				Icon = nil,
				Duration = 3,
			})
		end,
	})
