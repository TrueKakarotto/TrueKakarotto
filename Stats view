wait(4)

repeat
	task.wait()
until game.Players.LocalPlayer ~= nil and game.Players.LocalPlayer.Name ~= nil and game.Players.LocalPlayer:GetMouse() ~= nil

repeat
	task.wait()
until game:IsLoaded()


local function stoprunning()
	while true do
	end 
end
if  game.PlaceId == 5151400895 and game.PlaceId == 3311165597 and game.PlaceId == 3608496430 then
	return stoprunning()
else
	function notify(Text)
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Script",
			Text = Text,
			Icon = nil,
			Duration = 3,
		})
	end

	local vu = game:GetService("VirtualUser")
	game:GetService("Players").LocalPlayer.Idled:connect(function()
		vu:Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
		wait(1)
		vu:Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
	end)

	if game:FindFirstChild("DBU Prefmium") then
		return notify("Already Loaded")
	end
	local MainFolder = Instance.new("Folder", game)
	MainFolder.Name = "DBU Premium"

	spawn(function()
		if game:GetService("CoreGui"):FindFirstChild("PlayerList") then
			game:GetService("CoreGui"):FindFirstChild("PlayerList"):Destroy()
		end
		
		function format_number(number)
			local suffixes = {"", "K", "M", "B", "T", "QD"}
			local suffix_index = 1
		
			while math.abs(number) >= 1000 and suffix_index < #suffixes do
				number = number / 1000.0
				suffix_index = suffix_index + 1
			end
		
			return string.format("%.2f%s", number, suffixes[suffix_index])
		end
		
		local player = game:GetService("Players").LocalPlayer
		local Main = Instance.new("ScreenGui")
		local LeaderBoard = Instance.new("Frame")
		local Label = Instance.new("TextLabel")
		local Underline = Instance.new("Frame")
		local UICorner = Instance.new("UICorner")
		local ScrollingFrame = Instance.new("ScrollingFrame")
		local listLayout = Instance.new("UIListLayout")
		
		Main.Name = "Main"
		Main.Parent = game:GetService("CoreGui")
		
		LeaderBoard.Name = "LeaderBoard"
		LeaderBoard.Parent = Main
		LeaderBoard.BackgroundColor3 = Color3.fromRGB(44, 44, 44)
		LeaderBoard.BackgroundTransparency = 0.200
		LeaderBoard.BorderColor3 = Color3.fromRGB(0, 0, 0)
		LeaderBoard.BorderSizePixel = 0
		LeaderBoard.Position = UDim2.new(0.828558645, 0, 0.05, 0)
		LeaderBoard.Size = UDim2.new(0, 300, 0, 433)
		
		Label.Name = "Label"
		Label.Parent = LeaderBoard
		Label.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		Label.BackgroundTransparency = 1.000
		Label.BorderColor3 = Color3.fromRGB(0, 0, 0)
		Label.BorderSizePixel = 0
		Label.Position = UDim2.new(-0.00113967364, 0, -0.00239559473, 0)
		Label.Size = UDim2.new(0, 300, 0, 26)
		Label.Font = Enum.Font.SourceSans
		Label.Text = "LeaderBoard"
		Label.TextColor3 = Color3.fromRGB(255, 255, 255)
		Label.TextScaled = true
		Label.TextSize = 14.000
		Label.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
		Label.TextWrapped = true
		
		Underline.Name = "Underline"
		Underline.Parent = LeaderBoard
		Underline.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Underline.BorderColor3 = Color3.fromRGB(0, 0, 0)
