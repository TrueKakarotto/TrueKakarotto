	local AutoBlocking = Tab1:CreateToggle({
		Name = "Auto-Block",
		CurrentValue = false,
		Flag = "Togglepoaaa",
		Callback = function(Value)
			getgenv().AutoBlock = Value
			while getgenv().AutoBlock == true do
				task.wait()
				if getgenv().AutoBlock ~= true then
					repeat
						task.wait()
						game:GetService("ReplicatedStorage").Package.Events.block:InvokeServer(false)
					until game:GetService("Players").LocalPlayer.Status.Blocking.Value ~= true
				end
				break
			end
			repeat
				task.wait()
				game:GetService("ReplicatedStorage").Package.Events.block:InvokeServer(true)
					
				
			until getgenv().AutoBlock == false
		end,
	})

